# ===========================================
# .env.example - All supported formats
# ===========================================

# Basic key=value
APP_NAME=MyApp
APP_DEBUG=true

# Empty value
EMPTY_VALUE=

# Values with equals sign
DATABASE_URL=mysql://user:pass@localhost/db?charset=utf8

# Double quotes (preserves spaces, supports escapes)
APP_DESCRIPTION="My awesome application"
WELCOME_MESSAGE="Hello \"World\""

# Single quotes (no escape processing)
REGEX_PATTERN='[a-zA-Z0-9_]+'

# Inline comments (space before # required)
API_KEY=secret123 # this comment is ignored
SECRET="value with # hash inside" # this is ignored

# Whitespace is trimmed
  SPACED_KEY  =  trimmed value

# ===========================================
# Invalid examples (will throw exceptions):
# ===========================================
# 123_INVALID=value    # starts with number
# INVALID-KEY=value    # contains hyphen
# INVALID KEY=value    # contains space
